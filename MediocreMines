local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mediocre Mines", "GrapeTheme")

--MAIN
local Main = Window:NewTab("Visual")
local MainSection = Main:NewSection("Ores")



MainSection:NewToggle("Aluminium", "Aluminium esp", function(state)
    if state then
        for i, v in pairs(workspace.Mine:GetChildren()) do 
            if v.Name == "Aluminum" then
                local a = Instance.new("Highlight")
                a.Parent = v
            end
        end
    else
        for i, v in pairs(workspace.Mine:GetChildren()) do 
            if v.Name == "Aluminum" then
                v.Highlight:Destroy()
            end
        end
    end
end)

MainSection:NewToggle("Amethyst", "Amethyst esp", function(state)
